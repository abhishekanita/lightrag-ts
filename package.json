    "csv-stringify": "^6.5.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "exceljs": "^4.4.0",
    "fast-csv": "^5.0.2",
    "fast-xml-parser": "^5.0.9",
    "fetch": "^1.1.0",
    "fs-extra": "^11.3.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.14.2",
    "monodb": "^1.5.1",
    "openai": "^4.87.3",
    "p-limit": "^3.1.0",
    "papa": "^0.0.0",
